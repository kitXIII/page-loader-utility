{
  "name": "kit-page-loader",
  "version": "0.0.16",
  "description": "Page load utility",
  "bin": {
    "page-loader": "dist/bin/page-loader.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "watch": "jest --watch",
    "babel-node": "babel-node",
    "eslint": "eslint",
    "build": "NODE_ENV=production babel src --out-dir dist --source-maps inline",
    "prepublishOnly": "rm -rf dist && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kitXIII/project-lvl3-s310.git"
  },
  "keywords": [
    "load",
    "page"
  ],
  "author": "Nikita Konovalov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kitXIII/project-lvl3-s310/issues"
  },
  "homepage": "https://github.com/kitXIII/project-lvl3-s310#readme",
  "engines": {
    "node": "10"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/**"
    ],
    "testPathIgnorePatterns": [
      "__fixtures__"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "jest": "^25.5.3",
    "jest-cli": "^25.5.3",
    "nock": "^9.6.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "commander": "^4.1.1",
    "debug": "^4.1.1",
    "listr": "^0.14.3",
    "lodash": "^4.17.15"
  }
}
